{
  "name": "@shopickup/adapters-gls",
  "version": "1.0.0",
  "description": "GLS shipping carrier adapter for Shopickup (Hungary & Eastern Europe)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest --run",
    "dev": "tsc -w -p tsconfig.json",
    "codegen": "openapi-typescript ../../carrier-docs/hu-gls/hu-gls.openapi.yaml --output ./src/types/generated.ts"
  },
  "peerDependencies": {
    "@shopickup/core": "^1.0.0"
  },
  "devDependencies": {
    "@shopickup/core": "workspace:*",
    "@stoplight/prism-http": "^5.0.0",
    "axios": "^1.6.0",
    "openapi-typescript": "^6.7.0",
    "typescript": "^5.4.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^4.3.5"
  },
  "keywords": [
    "shipping",
    "carrier",
    "gls",
    "adapter",
    "logistics",
    "hungary",
    "eastern-europe",
    "pickup-points",
    "apm"
  ],
  "author": "Shopickup Contributors",
  "license": "MIT"
}
