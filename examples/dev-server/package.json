{
    "name": "shopickup-dev-server-example",
    "version": "1.0.0",
    "private": true,
    "author": "David Fodor",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "start": "node dist/server.js",
        "dev": "tsx src/server.ts --watch",
        "test": "vitest --run",
        "test:watch": "vitest",
        "kill:dev": "pids=$(lsof -t -iTCP:3000 -sTCP:LISTEN); [ -n \"$pids\" ] && kill $pids"
    },
     "dependencies": {
          "@fastify/cors": "^11.2.0",
          "@fastify/swagger": "^9.6.1",
          "@fastify/swagger-ui": "^5.2.4",
          "@shopickup/adapters-foxpost": "link:../../packages/adapters/foxpost",
          "@shopickup/adapters-mpl": "link:../../packages/adapters/MPL",
          "@shopickup/core": "link:../../packages/core",
          "axios": "^1.6.0",
          "dotenv": "^16.4.7",
          "fastify": "^5.7.1",
          "zod": "^4.3.5"
      },
     "devDependencies": {
         "@shopickup/core": "link:../../packages/core",
         "@types/node": "^25.0.9",
         "tsx": "^4.21.0",
         "vitest": "^4.0.17"
     }
}